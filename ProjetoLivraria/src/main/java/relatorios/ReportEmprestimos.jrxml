	<queryString language="SQL">
		<![CDATA[SELECT emprestimo.id_emprestimo,
	cliente.nome,
	livro.titulo,
	emprestimo.data_emprestimo,
	emprestimo.data_devolucao,
	emprestimo.id_cliente,
	cliente.sobrenome
FROM cliente,
	emprestimo,
	livro
WHERE 
	  emprestimo.id_cliente = cliente.id_cliente 
	 AND emprestimo.id_livro = livro.id_livro 
GROUP BY emprestimo.id_cliente,
	emprestimo.id_emprestimo,
	livro.id_livro,
